@model IEnumerable<ImgServiceWebApplication.Models.Photo>

@{
    ViewBag.Title = "Photos";
}

<h2>Photos</h2>


@foreach (var item in Model)
{
    <div class="col-xs-18 col-sm-6 col-md-3">
        <div class="thumbnail">
            <img src="@Url.Content(item.ImageRelativePath)" alt="" class="img-circle" width="242" height="200">
            <div class="caption">
                <h4>@item.Name</h4>
                <p>year: @item.Year Month:@item.Month</p>
                <p><a href="#" class="btn btn-info btn-xs" role="button">Button</a> <a href="#" class="btn btn-danger btn-xs" role="button">Button</a></p>
            </div>
        </div>
    </div>
}

